using System;

class MagicCannon
{
    static void Main()
    {
        for (int i = 1; i <= 100; i++)
        {
            if (i % 3 == 0 && i % 5 == 0)
                Console.WriteLine($"{i}: Fireball & Lightning");
            else if (i % 3 == 0)
                Console.WriteLine($"{i}: Fireball");
            else if (i % 5 == 0)
                Console.WriteLine($"{i}: Lightning");
            else
                Console.WriteLine($"{i}: Normal shot");
        }
    }
}
